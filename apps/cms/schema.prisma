// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model Action {
  id              String           @id @default(cuid())
  title           String           @default("")
  slug            String           @unique @default("")
  keywords        String           @default("")
  resume          String           @default("")
  content         Json             @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  image_id        String?
  image_filesize  Int?
  image_width     Int?
  image_height    Int?
  image_extension String?
  date            DateTime         @postgresql.Date
  publishedAt     DateTime?        @updatedAt
  status          ActionStatusType @default(published)
}

model Company {
  id           Int    @id
  title        String @default("")
  address      String @default("")
  phone        String @default("")
  email        String @default("")
  facebookUrl  String @default("")
  instagramUrl String @default("")
  youtubeUrl   String @default("")
}

model Event {
  id              String          @id @default(cuid())
  title           String          @default("")
  slug            String          @unique @default("")
  keywords        String          @default("")
  resume          String          @default("")
  content         Json            @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  image_id        String?
  image_filesize  Int?
  image_width     Int?
  image_height    Int?
  image_extension String?
  link            String          @default("")
  workload        Int
  date            DateTime        @postgresql.Date
  locale          String          @default("")
  publishedAt     DateTime?       @updatedAt
  status          EventStatusType @default(published)
}

model History {
  id           Int    @id
  titleOne     String @default("")
  contentOne   String @default("")
  titleTwo     String @default("")
  contentTwo   String @default("")
  titleThree   String @default("")
  contentThree String @default("")
  titleFour    String @default("")
  contentFour  String @default("")
  titleFive    String @default("")
  contentFive  String @default("")
}

model HomeSection {
  id              Int     @id
  title           String  @default("")
  content         String  @default("")
  image_id        String?
  image_filesize  Int?
  image_width     Int?
  image_height    Int?
  image_extension String?
}

model NewsletterList {
  id          String    @id @default(cuid())
  email       String    @unique @default("")
  publishedAt DateTime? @updatedAt
}

model Project {
  id              String            @id @default(cuid())
  title           String            @default("")
  slug            String            @unique @default("")
  keywords        String            @default("")
  content         Json              @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  image_id        String?
  image_filesize  Int?
  image_width     Int?
  image_height    Int?
  image_extension String?
  link            String            @default("")
  startDate       DateTime          @postgresql.Date
  endDate         DateTime          @postgresql.Date
  researchers     Researcher[]      @relation("Project_researchers")
  researchArea    ResearchArea?     @relation("Project_researchArea", fields: [researchAreaId], references: [id])
  researchAreaId  String?           @map("researchArea")
  publishedAt     DateTime?         @updatedAt
  status          ProjectStatusType @default(published)

  @@index([researchAreaId])
}

model Publication {
  id             String                @id @default(cuid())
  title          String                @default("")
  slug           String                @unique @default("")
  keywords       String                @default("")
  resume         String                @default("")
  content        Json                  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  link           String                @default("")
  date           DateTime              @postgresql.Date
  researchers    Researcher[]          @relation("Publication_researchers")
  researchArea   ResearchArea?         @relation("Publication_researchArea", fields: [researchAreaId], references: [id])
  researchAreaId String?               @map("researchArea")
  magazine       String                @default("")
  doi            String                @default("")
  license        String                @default("")
  publishedAt    DateTime?             @updatedAt
  status         PublicationStatusType @default(published)

  @@index([researchAreaId])
}

model ResearchArea {
  id              String                 @id @default(cuid())
  title           String                 @default("")
  resume          String                 @default("")
  content         Json                   @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  image_id        String?
  image_filesize  Int?
  image_width     Int?
  image_height    Int?
  image_extension String?
  icon_id         String?
  icon_filesize   Int?
  icon_width      Int?
  icon_height     Int?
  icon_extension  String?
  projects        Project[]              @relation("Project_researchArea")
  publications    Publication[]          @relation("Publication_researchArea")
  publishedAt     DateTime?              @updatedAt
  status          ResearchAreaStatusType @default(published)
}

model Researcher {
  id           String               @id @default(cuid())
  name         String               @default("")
  projects     Project[]            @relation("Project_researchers")
  publications Publication[]        @relation("Publication_researchers")
  publishedAt  DateTime?            @updatedAt
  status       ResearcherStatusType @default(published)
}

model TeamMember {
  id              String               @id @default(cuid())
  name            String               @default("")
  role            String               @default("")
  image_id        String?
  image_filesize  Int?
  image_width     Int?
  image_height    Int?
  image_extension String?
  link            String               @default("")
  publishedAt     DateTime?            @updatedAt
  status          TeamMemberStatusType @default(published)
}

model User {
  id          String    @id @default(cuid())
  name        String    @default("")
  email       String    @unique @default("")
  password    String
  publishedAt DateTime? @updatedAt
}

model ResearchSection {
  id      Int    @id
  title   String @default("")
  content String @default("")
}

model TeamSection {
  id      Int    @id
  title   String @default("")
  content String @default("")
}

model HistorySection {
  id      Int    @id
  title   String @default("")
  content String @default("")
}

model PublicationsSection {
  id      Int    @id
  title   String @default("")
  content String @default("")
}

model EventsSection {
  id      Int    @id
  title   String @default("")
  content String @default("")
}

model ActionsSection {
  id      Int    @id
  title   String @default("")
  content String @default("")
}

model ProjectsSection {
  id      Int    @id
  title   String @default("")
  content String @default("")
}

enum ActionStatusType {
  published
  draft
}

enum EventStatusType {
  published
  draft
}

enum ProjectStatusType {
  published
  draft
}

enum PublicationStatusType {
  published
  draft
}

enum ResearchAreaStatusType {
  published
  draft
}

enum ResearcherStatusType {
  published
  draft
}

enum TeamMemberStatusType {
  published
  draft
}
